{
  "name": "@lindsor/tscache",
  "version": "0.14.0",
  "description": "",
  "author": "Marcelo Luz <marcelo.luz@lindsor.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lindsor/tscache.git"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "build": "rm -rf dist && tsc -b tsconfig.json tsconfig.cjs.json tsconfig.types.json && npm run esmPackageJson",
    "esmPackageJson": "node ./bin/esm-package-json.mjs",
    "test": "jest",
    "test:ci": "npm run test -- --ci --reporters=default --reporters=jest-junit --coverageReporters=cobertura",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm version minor -m \"Bump version v%s\"",
    "postpublish": "git push && git push --tags"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "klona": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "jest-junit.xml"
  },
  "files": [
    "dist"
  ]
}
