{
  "name": "@lindsor/tscache",
  "version": "0.4.0",
  "description": "",
  "author": "Marcelo Luz <marcelo.luz@lindsor.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lindsor/tscache.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -b tsconfig.json tsconfig.cjs.json tsconfig.types.json && npm run esmPackageJson",
    "esmPackageJson": "node ./bin/esm-package-json.mjs",
    "test": "jest",
    "prepublish": "npm run build",
    "postpublish": "npm version minor -m \"Bump version v%s\" && git push && git push --tags"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "klona": "^2.0.6",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist"
  ]
}
